{
    "name": "composaic-demo",
    "version": "0.1.1",
    "description": "Example projects for Composaic",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/jarecsni/composaic-demo.git"
    },
    "scripts": {
        "api": "node ./server.js",
        "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
        "dev": "vite",
        "test": "jest",
        "test:watch": "jest --watch",
        "prettier": "prettier --write .",
        "build": "npm run prettier && vite build",
        "build:watch": "vite build --watch",
        "preview": "vite preview"
    },
    "keywords": [
        "nodejs",
        "package",
        "npm",
        "javascript",
        "typescript"
    ],
    "author": {
        "name": "Johnny Jarecsni",
        "email": "johnny.jarecsni@icloud.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.5",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@eslint/compat": "^1.0.3",
        "@eslint/js": "^9.4.0",
        "@originjs/vite-plugin-federation": "^1.3.5",
        "@rollup/plugin-alias": "^5.1.0",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "babel-plugin-module-resolver": "^5.0.2",
        "concurrently": "^8.2.2",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.2",
        "globals": "^15.3.0",
        "jest": "^29.5.0",
        "prettier": "^3.3.1",
        "sass": "^1.77.8",
        "typescript": "^5.0.4",
        "typescript-eslint": "^7.12.0",
        "vite": "^5.2.0"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.25.2",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4",
        "composaic": "^0.4.11",
        "express": "^4.19.2",
        "mitt": "^3.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.25.0",
        "runtypes": "^6.7.0"
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
            "\\.(css|less|scss)$": "<rootDir>/test/mocks/styleMock.js"
        }
    }
}
